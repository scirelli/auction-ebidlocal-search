function replaceText(textId, text)
{
  if (document.getElementById)
  {
    var textval=document.getElementById(textId);
    if (textval)
    {
      if (textval.value)
      {
	textval.value=text;
      }
      else //if (textval.innerHTML)
      {
	textval.innerHTML=text;
      }
    }
  }
}


