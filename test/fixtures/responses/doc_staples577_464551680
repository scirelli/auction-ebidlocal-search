function subform(rnum){
	document.rform.rnum.value=rnum;
	document.rform.scrolly.value = (document.all)?document.body.scrollTop:window.pageYOffset; 
	document.rform.submit();
}

function subwatchform(rnum){
	document.rform.rnum.value=rnum;
	document.rform.watchitem.value=rnum;
	document.rform.scrolly.value = (document.all)?document.body.scrollTop:window.pageYOffset; 
	document.rform.submit();
}

function subbidform(rnum){
	var df = 0;
	for (var i = 0; i < document.bidform.elements.length; i++) {
		var e = document.bidform.elements[i];
		if (e.type == "text" && e.value != "") 
		{df = 1; break;}
	} 
	if (df == 0)
	{
		alert ("No bid amount entered");
		return;
	}
	document.bidform.autoconfirm.value=rnum;
	document.bidform.submit();
}

function subfillform(rnum,bidamount){
	var rmax = "m";
	rmax += rnum;
	for (var i = 0; i < document.bidform.elements.length; i++) {
		var e = document.bidform.elements[i];
		if (e.name == rnum) {
			if (e.value == "")
			{e.value = bidamount;}
			else
			{e.value = "";}
			break;		
		} 
		if (e.name == rmax) {
			if (e.value == "")
			{e.value = bidamount;}
			else
			{e.value = "";}
			break;		
		} 
	} 
}

