var timers = [];
var tdifference; 
var tcounter = 0;

function ResetCounter() {
  tcounter = 0;
}

function RefreshBids() {
  tcounter++;
  if (tcounter == 50) {
    clearInterval(RefreshBids);
    alert("Auto-refresh has been halted");
    document.rform.submit();
  }  
  var xmlhttp = new XMLHttpRequest();
  xmlhttp.onreadystatechange = function () {
    if (xmlhttp.readyState == XMLHttpRequest.DONE) {
      if (xmlhttp.status == 200) {
        var data = JSON.parse(xmlhttp.responseText);
        const keys = Object.keys(data.items);
        for (const item of keys) {
          const details = document.getElementById(item);
          if (details != null) {
            var childElements = details.children;
            const nbsp = '&nbsp;';
            for (var k = 0; k < childElements.length; k++) {
              if (childElements[k].classList.contains("bids")) {
                var span_id = item + "_bids";
                span = document.getElementById(span_id);
                span.innerHTML = data.items[item].bids;
              }
              else if (childElements[k].classList.contains("highbidder")) {
                var span_id = item + "_highbidder";
                span = document.getElementById(span_id);
                span.innerHTML = data.items[item].high_bidder;
              }
              else if (childElements[k].classList.contains("currentamount")) {
                var span_id = item + "_currentprice";
                span = document.getElementById(span_id);
                span.innerHTML = data.items[item].current_price;
              }
              else if (childElements[k].classList.contains("nextbidrequired")) {
                var span_id = item + "_nextrequired";
                var currentstatus = data.items[item].status;
                span = document.getElementById(span_id);
                span.innerHTML = data.items[item].next_required;
                if (data.items[item].next_required != "&nbsp;" && data.items[item].next_required != " ") {
                  var a1 = "<a href=javascript:subfillform('";
                  var a2 = "','";
                  var a3 = "')>";
                  var a4 = "</a>";
                  var a5 = a1 + item + a2 + data.items[item].next_required + a3 + data.items[item].next_required + a4;
                  span.innerHTML = a5;
                }
                if (currentstatus == "ended") {
                  span.innerHTML = nbsp;
                }
              }
              else if (childElements[k].classList.contains("yourbid")) {
                var span_id = item + "_yourbid";
                var currentstatus = data.items[item].status;
                span = document.getElementById(span_id);
                if (currentstatus == "ended") {
                  span.innerHTML = nbsp;
                }
              }
              else if (childElements[k].classList.contains("yourmaximum")) {
                var span_id = item + "_yourmax";
                var currentstatus = data.items[item].status;
                span = document.getElementById(span_id);
                if (currentstatus == "ended") {
                  span.innerHTML = nbsp;
                }
                var span_id = item + "_status";
                span = document.getElementById(span_id);
                span.innerHTML = nbsp;
                if (currentstatus != "open" && currentstatus != "ended") {
                  if (currentstatus == "extended") {
                    currentstatus = "<font color=red><i>" + currentstatus + "</i></font>";
                  }
                  span.innerHTML = currentstatus;
                }
                var span_id = item + "_endtime";
                span = document.getElementById(span_id)
                var basesec = document.getElementsByName("baseseconds")[0].value;
                var end_time_countdown = parseInt(basesec) + parseInt(data.items[item].end_time); 
                var item_message = "ended";
                if (data.items[item].status == "open" || data.items[item].status == "extended") {
                  item_message = "closing";
                }
                clearInterval(timers[span_id])
                createTimer(span_id, end_time_countdown, item_message);
              }
            }
          }   
        }
      }
    }
  };
    var url = "/cgi-bin/v6.cgi?a=getBids&c=" + document.getElementsByName("client")[0].value + "&event=" + document.getElementsByName("auction")[0].value + '&contents=' + document.getElementsByName("contents")[0].value;

    xmlhttp.open("GET", url, true);
    xmlhttp.send();
}

function LeadingZero(Time) {
  return (Time < 10) ? "0" + Time : + Time;
}

function createTimer(timer_id, countDownDate, endText) {
  timers[timer_id] = setInterval(function () {
    var ndate = new Date();
    var nbaseseconds  = Math.round(ndate.getTime() / 1000);
    var distance = countDownDate - nbaseseconds - tdifference;
    var hours = Math.floor((distance % (60 * 60 * 24)) / (60 * 60));
    var minutes = Math.floor((distance % (60 * 60)) / 60);
    var seconds = Math.floor(distance % 60);
    document.getElementById(timer_id).innerHTML = "<b>" + LeadingZero(hours) + ":" + LeadingZero(minutes) + ":" + LeadingZero(seconds) + "</b>";
    if (distance < 0) {
        document.getElementById(timer_id).innerHTML = "<b>" + endText + "</b>";
    }
  }, 1000);
}

window.onload = (event) => {
  AutoRefresh();
};

function AutoRefresh() {
  var tdate = new Date();
  var tsecond = Math.round(tdate.getTime() / 1000);
  var basesec = document.getElementsByName("baseseconds")[0].value;
  var currsec = document.getElementsByName("currentsecond")[0].value;
  var arvalue = document.getElementsByName("arvalue")[0].value;
  var scrollval = document.getElementsByName("scrollval")[0].value;
  var arrefresh = parseInt(arvalue);
  tdifference = parseInt(basesec) + parseInt(currsec) - parseInt(tsecond);
  window.scrollTo(0,scrollval);
  if (document.getElementsByName("auto_refresh")[0].value == 1) {
    RefreshBids();
    setInterval(RefreshBids, arrefresh);
    return true;
  }
  return false;
}
