function searchtest()
{
	if (document.searchform.atype.value == 'EVENT')
	{
		return true;
	}
	document.searchform1.atype.value = document.searchform.atype.value;
	document.searchform1.stype.value = document.searchform.stype.value;
	document.searchform1.keyword.value = document.searchform.keyword.value;
	document.searchform1.submit();
	return false;
}

